<!DOCTYPE html>
<html>
<head>

    <%- include('../header'); %>

</head>
<body>

<%- include('navbar'); %>

<div class="card">
    <div class="card-header">
        Users
    </div>
    <div class="card-body">
        <table class="table table-striped">
            <thead>
            <tr>
                <th>id</th>
                <th>age</th>
                <th>level</th>
                <th>sex</th>
                <th>tests</th>
            </tr>
            </thead>
            <tbody>
            <% users.forEach(function(entry) { %>
            <tr>
                <td><a href="/users/<%= entry.id %>"><%= entry.id %></a></td>
                <td><%= entry.age %></td>
                <td><%= entry.level %></td>
                <td><%= entry.sex %></td>
                <td><a href="/users/<%= entry.id %>/tests">tests</a></td>
            </tr>
            <% }) %>
            </tbody>
        </table>

        <a class="btn btn-primary" href="/users/new" role="button">New User</a>

    </div>
</div>
</body>
</html>
